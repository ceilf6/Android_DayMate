        "@tsconfig/react-native": "^3.0.0",
        "@types/react": "^19.1.1",
        "@types/react-test-renderer": "^18.0.0",
        "babel-jest": "^29.2.1",
        "eslint": "^8.19.0",
        "jest": "^29.2.1",
        "patch-package": "^8.0.1",
        "prettier": "^2.4.1",
        "react-test-renderer": "18.2.0",
