{
    "name": "rn-calendar",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "ios:pods": "env -u all_proxy -u ALL_PROXY -u http_proxy -u HTTP_PROXY -u https_proxy -u HTTPS_PROXY -u no_proxy -u NO_PROXY SSL_CERT_FILE=/private/etc/ssl/cert.pem NO_FLIPPER=1 pod install --no-repo-update --project-directory=ios",
        "ios:run": "env -u all_proxy -u ALL_PROXY -u http_proxy -u HTTP_PROXY -u https_proxy -u HTTPS_PROXY -u no_proxy -u NO_PROXY SSL_CERT_FILE=/private/etc/ssl/cert.pem NO_FLIPPER=1 react-native run-ios",
        "lint": "eslint .",
        "start": "react-native start",
        "test": "jest",
        "postinstall": "patch-package"
    },
    "dependencies": {
        "@react-native-async-storage/async-storage": "^1.19.5",
        "@react-navigation/native": "^6.1.9",
        "@react-navigation/native-stack": "^6.9.17",
        "date-fns": "^2.30.0",
        "react": "18.2.0",
